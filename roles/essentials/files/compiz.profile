[move]
s0_lazy_positioning = true

[opengl]
s0_texture_filter = 0
s0_lighting = true

[workarounds]
s0_legacy_fullscreen = true
s0_firefox_menu_fix = true
s0_force_glx_sync = true

[switcher]
s0_focus_on_switch = true
s0_zoom = 5,000000
s0_icon_only = true
s0_auto_rotate = true

[put]
s0_put_next_output_key = <Shift><Alt>Left
s0_put_previous_output_key = <Shift><Alt>Right

[scale]
s0_overlay_icon = 1
s0_initiate_edge = BottomLeft

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;copytex;imgjpeg;maximumize;decor;gnomecompat;grid;imgpng;mousepoll;move;place;put;regex;resize;session;text;wall;workspacenames;animation;commands;expo;shift;workarounds;fade;scale;showdesktop;
s0_outputs = 1366x768+0+0;
s0_show_desktop_edge = BottomRight
s0_hsize = 2
s0_vsize = 2

[composite]
s0_refresh_rate = 50

[grid]
s0_bottom_edge_action = 2
s0_outline_color = #eeeeee9f
s0_fill_color = #7979794f

[expo]
s0_expo_edge = TopRight
s0_expo_immediate_move = true
s0_hide_docks = true
s0_mipmaps = true
s0_selected_color = #eeeeeeff
s0_reflection = true

[place]
s0_mode = 1

[shift]
s0_next_all_key = <Alt>Tab
s0_prev_all_key = <Shift><Alt>Tab

[decor]
s0_command = /usr/bin/gtk-window-decorator --metacity-theme Numix --replace
s0_mipmap = true

[workspacenames]
s0_names = Navegação;Desenvolvimento;Design;Virtualização;
s0_bold_text = false
s0_text_placement = 1

