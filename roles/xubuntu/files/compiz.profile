[place]
s0_mode = 1

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;copytex;imgjpeg;maximumize;decor;gnomecompat;grid;imgpng;mousepoll;move;place;put;regex;resize;resizeinfo;session;text;thumbnail;titleinfo;wall;workspacenames;animation;commands;expo;shift;workarounds;fade;scale;showdesktop;
s0_outputs = 1920x1080+1920+0;1920x1080+0+0;
s0_show_desktop_edge = BottomRight
s0_hsize = 2
s0_vsize = 2

[opengl]
s0_texture_filter = 2
s0_lighting = true
s0_texture_compression = true

[switcher]
s0_focus_on_switch = true
s0_zoom = 5,000000
s0_icon_only = true
s0_auto_rotate = true

[wobbly]
s0_spring_k = 10,000000

[decor]
s0_command = /usr/bin/gtk-window-decorator --metacity-theme Numix --replace
s0_mipmap = true

[resizeinfo]
s0_always_show = true
s0_text_color = #ffffffff
s0_gradient_1 = #000000ff
s0_gradient_2 = #000000ff
s0_gradient_3 = #000000ff
s0_outline_color = #000000ff

[move]
s0_lazy_positioning = true

[workspacenames]
s0_names = Navegação;Desenvolvimento;Design;Virtualização;
s0_bold_text = false
s0_text_placement = 1

[put]
s0_put_next_output_key = <Shift><Alt>Left
s0_put_previous_output_key = <Shift><Alt>Right

[fade]
s0_fade_mode = 1

[expo]
s0_expo_edge = TopRight
s0_expo_immediate_move = true
s0_hide_docks = true
s0_mipmaps = true
s0_multioutput_mode = 0
s0_selected_color = #eeeeeeff
s0_reflection = true

[shift]
s0_next_all_key = <Alt>Tab
s0_prev_all_key = <Shift><Alt>Tab

[addhelper]
s0_ononinit = true
s0_brightness = 90
s0_saturation = 60

[workarounds]
s0_legacy_fullscreen = true
s0_firefox_menu_fix = true
s0_force_glx_sync = true

[scale]
s0_overlay_icon = 1
s0_initiate_edge = BottomLeft
s0_click_on_desktop = 1

[grid]
s0_bottom_edge_action = 2
s0_outline_color = #eeeeee9f
s0_fill_color = #7979794f

[thumbnail]
s0_border = 1
s0_thumb_color = #ffffff97
s0_mipmap = true

